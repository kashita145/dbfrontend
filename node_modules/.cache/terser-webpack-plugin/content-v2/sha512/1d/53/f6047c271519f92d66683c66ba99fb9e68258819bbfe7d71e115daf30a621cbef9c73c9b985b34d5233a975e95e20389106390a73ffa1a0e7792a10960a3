ordion defaultActiveKey=\"0\">\n      <Card>\n        <Card.Header className=\"card-header\">\n          <CustomToggle className=\"custom-btn-primary\" eventKey=\"1\">\n            Start Process\n          </CustomToggle>\n          <CustomToggle className=\"custom-btn-primary\" eventKey=\"0\">\n            <FaFilter /> Filter\n          </CustomToggle>\n        </Card.Header>\n        <Accordion.Collapse className=\"acc-body\" eventKey=\"1\">\n          <StartProcess processNames={processNames} serverNames={serverNames} />\n        </Accordion.Collapse>\n        <Accordion.Collapse className=\"acc-body\" eventKey=\"0\">\n          <Filters\n            filterProcesses={filterProcesses}\n            processes={processes}\n            processNames={processNames}\n            serverNames={serverNames}\n          />\n        </Accordion.Collapse>\n      </Card>\n    </Accordion>\n  );\n};\n\nexport default Filter;\n","import Navbar from \"react-bootstrap/Navbar\";\nconst MyNavbar = ({ text }) => {\n  return (\n    <Navbar>\n      <Navbar.Brand className=\"custom-brand\">\n        <span className=\"footer-text-main\">RTB</span> Dashboard\n      </Navbar.Brand>\n      <Navbar.Toggle />\n      <Navbar.Collapse className=\"justify-content-end\">\n        <Navbar.Text>\n          <a className=\"nav-link\" href=\"/login\">\n            {text}\n          </a>\n        </Navbar.Text>\n      </Navbar.Collapse>\n    </Navbar>\n  );\n};\n\nexport default MyNavbar;\n","import React from \"react\";\nimport { Icon } from \"@iconify/react\";\nimport heartFilled from \"@iconify-icons/ant-design/heart-filled\";\n\nconst Footer = () => {\n  return (\n    <div className=\"footer\">\n      <span className=\"footer-text-main\">RTB </span>Dashboard\n      <br />\n      Made with <Icon icon={heartFilled} color=\"red\" /> by{\" \"}\n      <a href=\"https://github.com/archits581\">Archit Shinde</a>,{\" \"}\n      <a href=\"https://github.com/kashita145/\">Kashita Talreja</a> and{\" \"}\n      